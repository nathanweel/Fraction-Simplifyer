public class MyClass {
    public static void main(String args[]) {
        
      //change number
      double x = 10;
      
      //change number
      double y = 25;
      
      //change for differnt operation (add, sub, mult, div)
      String op = "div";
      
      
      
      
      //the primitive "x" always comes before "y"
      
      if (op.equals("add")){
          System.out.println(x + y);
          
      }else if (op.equals("sub")){
          System.out.println(x - y);
          
      }else if (op.equals("mult")){
          System.out.println(x * y);
          
      }else if (op.equals("div")){
          System.out.println(x / y);
      }
    }
}
