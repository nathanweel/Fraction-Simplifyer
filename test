public class MyClass {
    public static void main(String args[]) {
      int x=10;
      int y=25;
      int z=x+y;

      System.out.println("Sum of x+y = " + z);
      
       if (z == 69){
          System.out.println("hahaha " + z + " is a funny number");
      }else{
          System.out.println( z + " has no importance");
       }
    }
}
